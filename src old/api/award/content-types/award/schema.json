{
  "kind": "collectionType",
  "collectionName": "awards",
  "info": {
    "singularName": "award",
    "pluralName": "awards",
    "displayName": "Award"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "position": {
      "type": "biginteger",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true,
      "unique": true
    },
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "awards"
    },
    "listing_img": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "image": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "company": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "year": {
      "type": "biginteger",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true,
      "unique": false,
      "default": "2025"
    }
  }
}
