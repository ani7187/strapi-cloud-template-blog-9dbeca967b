    "bottom_panel": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::bottom-panel.bottom-panel"
    },
    "gallery": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
