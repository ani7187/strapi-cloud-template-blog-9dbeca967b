    "header": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "sections.description",
      "repeatable": false
    },
    "ambassador": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "sections.ambassador",
      "repeatable": false
    },
    "news": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
