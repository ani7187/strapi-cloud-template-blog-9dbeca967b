{
  "kind": "collectionType",
  "collectionName": "persons",
  "info": {
    "singularName": "person",
    "pluralName": "persons",
    "displayName": "Person"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "position": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true,
      "unique": true
    },
    "full_name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "person_position": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "team_category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::team-category.team-category"
    },
    "person_tag": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::person-position.person-position"
    },
    "bio": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "email": {
      "type": "email",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "phone_number": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "countries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::country.country"
    },
    "img": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
