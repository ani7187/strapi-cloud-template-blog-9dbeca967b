{
  "kind": "collectionType",
  "collectionName": "template_pages",
  "info": {
    "singularName": "template-page",
    "pluralName": "template-pages",
    "displayName": "Template page"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "slug": {
      "type": "uid",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "targetField": "title",
      "required": true
    },
    "header": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "sections.header-section",
      "repeatable": false,
      "required": true
    },
    "bottom_panel": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::bottom-panel.bottom-panel"
    },
    "sections": {
      "type": "dynamiczone",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "components": [
        "sections.award",
        "sections.header-section",
        "sections.futures",
        "sections.partners",
        "sections.success-story",
        "sections.product-solutions",
        "extended-sections.item-list",
        "extended-sections.item-list-with-btn",
        "extended-sections.related-content",
        "extended-sections.rich-content-markdown",
        "extended-sections.services-item-list",
        "extended-sections.scrollable-items",
        "sections.number",
        "shared.banner",
        "extended-sections.white-item-list-with-btn",
        "shared.video",
        "extended-sections.voices",
        "extended-sections.gallery",
        "sections.description",
        "extended-sections.timeline",
        "extended-sections.mission-vision"
      ]
    },
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "seo": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.seo",
      "repeatable": false
    },
    "include_sitemap": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true,
      "default": true
    }
  }
}
