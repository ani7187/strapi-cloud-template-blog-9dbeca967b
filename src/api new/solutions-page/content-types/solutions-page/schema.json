{
  "kind": "singleType",
  "collectionName": "solutions_pages",
  "info": {
    "singularName": "solutions-page",
    "pluralName": "solutions-pages",
    "displayName": "Solutions page"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "slug": {
      "type": "uid",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "header": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "sections.header-section",
      "repeatable": false,
      "required": true
    },
    "short_description": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.short-description",
      "repeatable": false
    },
    "btn": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.link-btn",
      "repeatable": true
    },
    "awards": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "sections.award",
      "repeatable": false
    },
    "statistic": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "sections.number",
      "repeatable": false
    },
    "success_story": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "sections.success-story",
      "repeatable": false
    },
    "banner": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.banner",
      "repeatable": false
    },
    "related_content": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::related-content.related-content"
    },
    "form": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "sections.form",
      "repeatable": false
    },
    "bottom_panel": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::bottom-panel.bottom-panel"
    },
    "seo": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.seo",
      "repeatable": false
    },
    "include_sitemap": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true,
      "default": true
    }
  }
}
