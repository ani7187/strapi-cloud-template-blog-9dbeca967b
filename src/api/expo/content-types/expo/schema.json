{
  "kind": "collectionType",
  "collectionName": "expos",
  "info": {
    "singularName": "expo",
    "pluralName": "expos",
    "displayName": "Expo"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "listing_title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": true
    },
    "slug": {
      "type": "uid",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "targetField": "listing_title",
      "required": true
    },
    "listing_summary": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "listing_img": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "header": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "sections.header-section",
      "repeatable": false,
      "required": true
    },
    "expo_type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::expo-type.expo-type"
    },
    "count_down_text": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "start_date": {
      "type": "datetime",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "end_date": {
      "type": "datetime",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "description": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "sections.description-simple",
      "repeatable": false
    },
    "location": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "required": false
    },
    "stand_location": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "numbers": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.number",
      "repeatable": true
    },
    "product_list": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "sections.product-list",
      "repeatable": false
    },
    "rich_content_list": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "sections.rich-content-list",
      "repeatable": false
    },
    "team": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "sections.people",
      "repeatable": false
    },
    "form": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "sections.form",
      "repeatable": false
    },
    "banner": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.banner",
      "repeatable": false
    },
    "bottom_panel": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::bottom-panel.bottom-panel"
    },
    "seo": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "shared.seo",
      "repeatable": false
    },
    "include_sitemap": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true,
      "default": true
    }
  }
}
